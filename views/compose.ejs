<%-include("partials/header");-%>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input class="form-control" type="text" id="postTitle" name="postTitle" autocomplete="off">
    <br>
    <label for="post">Post</label>
    <textarea class="form-control" id="post" rows="6" name="postContent"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="publish">Publish</button>
</form>

<%-include("partials/footer");-%>
